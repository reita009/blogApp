<%- include('../../partials/header.ejs') %> <%-
include('../../partials/navbar.ejs') %>

<div class="container">
  <hr />
  <h2>Usuário</h2>
  <a href="/admin/users/create" class="btn btn-success"> Criar novo usuário </a>
  <br />
  <br />
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>id</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user =>{ %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<%- include('../../partials/footer.ejs') %>
